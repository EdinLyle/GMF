{
  "name": "prompt-injection-manager",
  "version": "1.0.0",
  "description": "专业的提示词注入（Prompt Injection）安全测试与管理工具",
  "main": "app/dist/main/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run ensure-vite-server && npm run dev:main",
    "ensure-vite-server": "node scripts/ensure-vite-server.js",
    "dev:renderer": "cross-env NODE_ENV=development cd app/renderer && vite --host",
    "dev:main": "cross-env NODE_ENV=development npm run build:preload && nodemon --exec \"electron app/dist/main/main.js\"",
    "build": "npm run build:renderer && npm run build:main && npm run build:preload && npm run copy-public",
    "build:renderer": "cd app/renderer && vite build",
    "build:main": "tsc -p app/main/tsconfig.json",
    "build:preload": "tsc app/main/preload.ts --outDir app/main/dist --target ES2020 --module commonjs --moduleResolution node --esModuleInterop --skipLibCheck",
    "copy-public": "node scripts/copy-public.js",
    "preview": "cd app/renderer && vite preview",
    "lint": "eslint app --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "watch:main": "tsc -p app/main/tsconfig.json --watch",
    "postinstall": "",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux"
  },
  "keywords": [
    "prompt-injection",
    "security-testing",
    "ai-safety",
    "electron"
  ],
  "author": "Security Research Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.11",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sql.js": "^1.12.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.4.7"
  }
}
